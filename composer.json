{
    "name": "krissanwat/laravel-monaco",
    "description": "A Monaco editor package for Laravel",
    "keywords": [
        "spatie",
        "laravel-monaco"
    ],
    "homepage": "https://github.com/spatie/laravel-mailcoach-monaco",
    "authors": [
        {
            "name": "kriss",
            "email": "me@kriss.io",
            "homepage": "https://kriss.io",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^7.4",
        "ext-json": "*",
        "spatie/laravel-mailcoach": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://github.com/krissnawat/laravel-monaco"
        }
    ],
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachMonaco\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachMonaco\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Spatie\\MailcoachMonaco\\MailcoachMonacoServiceProvider"
            ]
        }
    }
}
